{
    "name": "sangamkatwal/sangam-toastr",
    "description": "A Laravel package for Toastr notifications (success, error, warning, info) with support for both session and ajax responses.",
    "keywords": ["laravel", "toastr", "notifications", "sangam"],
    "homepage": "https://github.com/sangamkatwal/sangam-toastr",
    "license": "MIT",
    "authors": [
        {
            "name": "Sangam Katwal",
            "email": "katwalsangam@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SangamToastr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SangamToastr\\ToastrServiceProvider"
            ]
        }
    }
}
